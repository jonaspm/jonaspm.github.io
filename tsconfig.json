{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/styles/*": ["src/styles/*"],
      "@/components/*": ["src/components/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/images/*": ["public/images/*"],
      "@/pages/*": ["src/pages/*"],
      "@/assets/*": ["src/assets/*"]
    }
  }
}
